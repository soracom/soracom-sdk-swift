{% if description %}
/** {{ description }} */
{% endif %}
public enum {{ enumName }}: {{ type }}, Codable {
    {% for enumCase in enums %}
    case {{ enumCase.name }} = {% if type == "String" %}"{% endif %}{{enumCase.value}}{% if type == "String" %}"{% endif %}
    {% endfor %}

    public static let cases: [{{ enumName }}] = [
      {% for enumCase in enums %}
      .{{ enumCase.name }},
      {% endfor %}
    ]
}
